pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat './Jenkins/Build/build.bat'
            }
        }
        stage('Push') {
            steps {
                bat './Jenkins/Push/push.bat'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
