pipeline {
    agent any
    
    environment {
        PASS = credentials('DockerHubPass')
    }

    stages {
        stage('Build') {
            steps {
                bat './Jenkins/1-Build/build.bat'
            }
        }
        stage('Push') {
            steps {
                bat './Jenkins/2-Push/push.bat'
            }
        }
        stage('Clean') {
            steps {
                bat './Jenkins/3-Clean/clean.bat'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
